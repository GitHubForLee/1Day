<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.robin.mybaseui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" 
        android:name="com.robin.mybaseui.app.MyApp"
         >
        <activity
            android:name="com.robin.mybaseui.LogoAct"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.robin.mybaseui.WelcomeAct"
            android:label="@string/app_name" >
           <intent-filter>
               <action android:name="com.robin.mybaseui.WelcomeAct" />
               <category android:name="android.intent.category.DEFAULT" />
           </intent-filter>
        </activity>
        <activity
            android:name="com.robin.mybaseui.MainAct"
            android:label="@string/app_name" >
           <intent-filter>
               <action android:name="com.robin.mybaseui.MainAct" />
               <category android:name="android.intent.category.DEFAULT" />
           </intent-filter>
        </activity>
        <activity
            android:name="com.robin.mybaseui.SettingAct"
            android:label="@string/app_name" >
           <intent-filter>
               <action android:name="com.robin.mybaseui.SettingAct" />
               <category android:name="android.intent.category.DEFAULT" />
           </intent-filter>
        </activity>
        
        
    </application>

</manifest>
